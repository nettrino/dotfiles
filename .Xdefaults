xterm *metaSendsEscape: true
Xcursor.size: 15

URxvt.saveLines: 100000
URxvt.scrollBar: false
URxvt.scrollBar_right: true
URxvt.geometry: 90x24
URxvt.foreground: #D8D8D8
URxvt.background: #000000
URxvt.secondaryScroll: true
URxvt.font: xft:Monospace:size=10:antialias=true
URxvt.letterSpace: -1
URxvt.internalBorder: 1
URxvt.skipBuildInGlyphs: true
URxvt.fading: 20
URxvt.fadeColor: black
URxvt.perl-ext-common: default, matcher
URxvt.urlLauncher: /usr/bin/x-www-browser
URxvt.matcher.button: 1
URxvt*iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! Tango theme by https://gist.github.com/menski/2176539
! Tango color palette
URxvt*background:         #000000
URxvt*foreground:         #d8d8d8
URxvt*cursorColor:        #d8d8d8
! foreground color for underline
URxvt*colorUL:            #8ae234
! line color for underline
URxvt*underlineColor:     #92659a
! black dark/light
URxvt*color0:             #2e3436
URxvt*color8:             #6e706b
! red dark/light
URxvt*color1:             #cc0000
URxvt*color9:             #ef2929
! green dark/light
URxvt*color2:             #4e9a06
URxvt*color10:            #84d831
! yellow dark/light
URxvt*color3:             #edd400
URxvt*color11:            #fce94f
! blue dark/light
URxvt*color4:             #3465a4
URxvt*color12:            #729fcf
! magenta dark/light
URxvt*color5:             #92659a
URxvt*color13:            #c19fbe
! cyan dark/light
URxvt*color6:             #07c7ca
URxvt*color14:            #63e9e9
! white dark/light
URxvt*color7:             #d3d7cf
URxvt*color15:            #eeeeec
